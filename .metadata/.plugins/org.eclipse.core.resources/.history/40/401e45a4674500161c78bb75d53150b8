if __name__ == '__main__':
    pass



class WithObject(object):
    
    def __init__(self, numero = 0):
        self.numero
    
    def __enter__(self):
        print("enter")
        self.__numero += 1
    
    def __exit__(self):
        self.__numero -= 1
        print("exit")