class WithObject(object):
    """context manager class"""
    def __init__(self):
        pass
    
    def __enter__(self):
        print("holiiis")
    def __exit__(self, type, value, traceback):
        print("chauuu")

if __name__ == '__main__':
    lista = [1,2,3,4,5]
    for elemento in lista:
        with WithObject():
            print(elemento)
            
    print(lista)



