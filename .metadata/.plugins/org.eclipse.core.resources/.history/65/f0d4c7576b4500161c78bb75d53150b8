def ejemplo():
    with open("../files.txt") as f:
        contenido = f.read()
        print(contenido)
    

def ejemplo2():
    with open("../NOEXISTE.txt") as f:
        contenido = f.read()
        print(contenido)
        
if __name__ == '__main__':
    ejemplo()

    



